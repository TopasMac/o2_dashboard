  "scripts": {
    "start": "react-scripts start",
    "start-clean": "kill -9 $(lsof -t -i:3000) || true && react-scripts start",
    "prestart": "rm -rf node_modules/.cache .eslintcache || true",
    "start:mobile": "HOST=127.0.0.1 PORT=3100 WDS_ALLOWED_HOSTS=all DANGEROUSLY_DISABLE_HOST_CHECK=true WDS_SOCKET_HOST=localhost WDS_SOCKET_PORT=3100 CHOKIDAR_USEPOLLING=true CHOKIDAR_INTERVAL=500 WATCHPACK_POLLING=true BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "build:dev": "react-scripts build --no-minify",
    "analyze": "npm run build:dev && source-map-explorer 'build/static/js/*.js'",
    "analyze-bundle": "npm run build:dev && npx webpack-bundle-analyzer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:build": "./src/tailwind_build/get-tailwind.sh",
    "build:tailwind": "npx tailwindcss -c src/tailwind_build/tailwind.config.js -i ./src/index.css -o ./src/tailwind_build/index.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
