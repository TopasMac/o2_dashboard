  "scripts": {
    "start": "react-scripts start",
    "start-clean": "kill -9 $(lsof -t -i:3000) || true && react-scripts start",
    "prestart": "rm -rf node_modules/.cache .eslintcache || true",
    "start:mobile": "HOST=0.0.0.0 PORT=3000 WDS_ALLOWED_HOSTS=all DANGEROUSLY_DISABLE_HOST_CHECK=true WDS_SOCKET_HOST=127.0.0.1 WDS_SOCKET_PORT=3000 CHOKIDAR_USEPOLLING=true CHOKIDAR_INTERVAL=500 WATCHPACK_POLLING=true BROWSER=google-chrome react-scripts start",
    "api:dev": "cd ../ && php -S 0.0.0.0:8001 -t public public/router.php",
    "api:prod": "bash -lc 'cd /home/ubuntu/owners2_main && (lsof -t -i:8001 | xargs -r kill -9) || true; APP_ENV=prod APP_DEBUG=0 php -S 0.0.0.0:8001 -t public'",
    "ngrok": "ngrok http 3000 --log=stdout | tee /tmp/ngrok.log & sleep 2 && NGROK_URL=$(grep -o 'https://[^ ]*ngrok-free.dev' /tmp/ngrok.log | tail -n1) && echo \"$NGROK_URL\" > /tmp/o2_ngrok_url.txt && echo '' && echo 'ðŸ“± MOBILE:' \"$NGROK_URL/m/dashboard\" && wait",
    "mobile:url": "bash -lc 'if [ -f /tmp/o2_ngrok_url.txt ] && [ -s /tmp/o2_ngrok_url.txt ]; then U=$(cat /tmp/o2_ngrok_url.txt); echo \"ðŸ“± MOBILE: ${U}/m/dashboard\"; else echo \"ðŸ“± MOBILE: (ngrok not running yet)\"; fi'",
    "dev:all": "concurrently -k -n API,WEB,NGROK \"npm run api:dev\" \"npm run start:mobile\" \"npm run ngrok\"",
    "build": "react-scripts build",
    "build:dev": "react-scripts build --no-minify",
    "analyze": "npm run build:dev && source-map-explorer 'build/static/js/*.js'",
    "analyze-bundle": "npm run build:dev && npx webpack-bundle-analyzer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tailwind:build": "./src/tailwind_build/get-tailwind.sh",
    "build:tailwind": "npx tailwindcss -c src/tailwind_build/tailwind.config.js -i ./src/index.css -o ./src/tailwind_build/index.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
